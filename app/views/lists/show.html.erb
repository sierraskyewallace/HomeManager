<h1> Your List - <%= @list.name %> </h1><br>



    <br>
<%= link_to 'Add Task', new_list_task_path(@list) %><br>

<% @list.tasks.each do |t| %><br>
    <%= link_to t.name, list_task_path(@list, t) %>
    <%= link_to 'Done', list_task_path(@list, t), method: :delete %>
    <% end %>
<br><br>


<%= link_to 'Edit List', edit_list_path(@list) %><br>
<%= link_to 'Delete List', list_path(@list), :method => :delete %>