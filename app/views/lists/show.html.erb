<div class="container"
<h5><%= @list.name %></h5>

<%= link_to 'Edit List', edit_list_path %>
<%= link_to 'Delete List', list_path(@list), method: :delete %>
<%= link_to 'All Lists', lists_path %>

    <p> <%= form_for ([@list, @task]) do |f| %>
        <%= f.text_field :name, placeholder: "Add Task" %>
        <p> Team Member: <%= f.collection_select :user, :user_id, User.all, :email %>
        <%= f. submit %><br>
      <% end %> </p><br>

      <%= render '/tasks/task'%><br>
      </div>

  
